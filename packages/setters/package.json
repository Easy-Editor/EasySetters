{
  "name": "@easy-editor/setters",
  "version": "0.0.6",
  "description": "Official setters library for EasyEditor - A collection of property setter components",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.min.js",
    "jsdelivr": "dist/index.min.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs"
      },
      "./styles.css": "./dist/styles.css"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/Easy-Editor/EasySetters",
  "license": "MIT",
  "author": "JinSo <kimjinso@qq.com>",
  "keywords": [
    "@easy-editor",
    "easyeditor",
    "low-code",
    "setter",
    "property-editor",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Easy-Editor/EasySetters",
    "directory": "packages/setters"
  },
  "bugs": {
    "url": "https://github.com/Easy-Editor/EasySetters/issues"
  },
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "format": "biome format --write .",
    "lint": "biome check .",
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/",
    "build:js": "rollup -c",
    "build:types": "pnpm types",
    "types": "npm-run-all -nl types:*",
    "types:src": "tsc --project tsconfig.build.json",
    "test-types": "tsc --project tsconfig.test.json"
  },
  "peerDependencies": {
    "@easy-editor/core": "*",
    "@types/react": "^18 || ^19",
    "@types/react-dom": "^18 || ^19",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/events": "^3.0.3",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "react-router-dom": "^7.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "shadcn": "^3.6.3",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@uiw/react-color-sketch": "^2.3.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  }
}
